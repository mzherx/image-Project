{
  "version": 2,
  "builds": [
    {
      "src": "../client/**",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "../client/dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
